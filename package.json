{
    "name": "adrenalink-beta",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build --turbopack",
        "start": "next start",
        "lint": "eslint . --fix",
        "prettyme": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "db:reset": "supabase db reset --linked --yes && supabase db push --linked --yes",
        "db:push": "supabase db push --linked --yes",
        "db:seed-all": "bun supabase/db/mock-berkley.ts && bun supabase/db/mock-tenerife.ts",
        "db:seed-simple": "npx ts-node supabase/db/mock-simple.ts",
        "db:seed-5": "npx ts-node supabase/db/mock-5.ts",
        "cf:get-admin": "bun run cloudflare/get-admin-bucket.ts",
        "cf:rm": "bun run cloudflare/rm-bucket.ts",
        "cf:seed-admin": "bun run cloudflare/seed-admin-bucket.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.917.0",
        "@clerk/nextjs": "^6.36.8",
        "@googlemaps/js-api-loader": "^2.0.1",
        "@headlessui/react": "^2.2.9",
        "@heroicons/react": "^2.2.0",
        "@hookform/resolvers": "^5.2.2",
        "@mux/mux-player-react": "^3.10.2",
        "@supabase/supabase-js": "^2.75.0",
        "@types/pg": "^8.15.5",
        "@vercel/analytics": "^1.5.0",
        "date-fns": "^4.1.0",
        "framer-motion": "^12.23.24",
        "lucide-react": "^0.545.0",
        "next": "^16.0.0",
        "next-themes": "^0.4.6",
        "postgres": "^3.4.7",
        "react": "19.1.0",
        "react-country-flag": "^3.1.0",
        "react-day-picker": "^9.13.0",
        "react-dom": "19.1.0",
        "react-easy-crop": "^5.5.6",
        "react-hook-form": "^7.65.0",
        "react-hot-toast": "^2.6.0",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@faker-js/faker": "^9.9.0",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "dotenv": "^17.2.3",
        "eslint": "^9",
        "eslint-config-next": "^16.0.0",
        "prettier": "^3.6.2",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
