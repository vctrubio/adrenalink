{
    "name": "adrenalink-beta",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build --turbopack",
        "start": "next start",
        "lint": "eslint . --fix",
        "prettyme": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "db:test-connection": "bun run drizzle/scripts/test-db-connection.ts",
        "db:seed": "bun run drizzle/scripts/students-seed.ts",
        "db:get-seeds": "bun run drizzle/scripts/get-seeds.ts",
        "db:clear": "bun run drizzle/scripts/clear.ts",
        "db:clear-meta": "bun run drizzle/scripts/clear-meta.ts",
        "db:workflow": "bun run db:generate && bun run db:migrate && bun run db:push",
        "db:reset": "bun run db:clear && bun run db:push",
        "db:fresh": "bun run db:clear-meta && bun run db:push"
    },
    "dependencies": {
        "@faker-js/faker": "^10.0.0",
        "@googlemaps/js-api-loader": "^2.0.1",
        "@headlessui/react": "^2.2.9",
        "@hookform/resolvers": "^5.2.2",
        "@supabase/supabase-js": "^2.75.0",
        "@types/pg": "^8.15.5",
        "@vercel/analytics": "^1.5.0",
        "dotenv": "^17.2.3",
        "drizzle-kit": "^0.31.5",
        "drizzle-orm": "^0.44.6",
        "lucide-react": "^0.545.0",
        "newrelic": "^13.5.0",
        "next": "15.5.4",
        "next-themes": "^0.4.6",
        "postgres": "^3.4.7",
        "react": "19.1.0",
        "react-country-flag": "^3.1.0",
        "react-dom": "19.1.0",
        "react-hook-form": "^7.65.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.5.4",
        "prettier": "^3.6.2",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
}
